{
    "optionalFeatures": {
        "acceptableAssertionUsage": true,
        "acceptableCameraUsage": true,
        "acceptableScreenSharingUsage": true,
        "aggressiveUserExperience": false,
        "attemptToFetchMajorUpgrade": true,
        "acceptableUpdatePreparingUsage": true,
        "disableNudgeForStandardInstalls": false,
        "honorFocusModes": true,
        "honorCycleTimersOnExit": true,
        "refreshSOFAFeedTime": 86400,
        "utilizeSOFAFeed": true,
        "attemptToCheckForSupportedDevice": true
    },
    "osVersionRequirements": [
        {
            "requiredMinimumOSVersion": "latest",
            "activelyExploitedCVEsMajorUpgradeSLA": 7,
            "activelyExploitedCVEsMinorUpdateSLA": 7,
            "nonActivelyExploitedCVEsMajorUpgradeSLA": 14,
            "nonActivelyExploitedCVEsMinorUpdateSLA": 14,
            "standardMajorUpgradeSLA": 28,
            "standardMinorUpdateSLA": 28,
        }
    ],
    "userExperience": {
        "allowGracePeriods": true,
        "allowMovableWindow": true,
        "nudgeMajorUpgradeEventLaunchDelay": 2,
        "nudgeMinorUpdateEventLaunchDelay": 2
    },
    "userInterface": {
        "showActivelyExploitedCVEs": true,
        "showDaysRemainingToUpdate": true,
        "showRequiredDate": true,
        "simpleMode": false,
        "requiredInstallationDisplayFormat": "MM/dd/yyyy",
    }
}

